{
  "name": "reactjs-express-docker-starter",
  "version": "1.0.0",
  "description": "a starter reactapp hosted via express, deployed as a docker container ",
  "main": "index.js",
  "scripts": {
    "lint": "esw app",
    "test": "jest --config=jest.config.json",
    "build:dev": "npm run clean && webpack --env.dev --watch",
    "build:prod": "npm run clean && webpack --env.prod -p",
    "serve": "nodemon server.js",
    "serve:hmr": "webpack-dev-server --config-name=client-hmr --env=dev",
    "clean": "rimraf dist"
  },
  "author": "Santanu Paul",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
    "chalk": "^2.3.0",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "node-env-file": "^0.1.8",
    "nodemon": "^1.14.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "jest": "^22.0.4",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.9",
    "precss": "^2.0.0",
    "raf": "^3.4.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
